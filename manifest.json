{
	"manifest_version": 2,
	"name": "Youtube Unshorten",
	"version": "1.0",
	"description": "redirects yotube shorts to regular player",
	"icons": {
		"48": "icons/noshort-48.png",
		"96": "icons/noshort-96.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*.youtube.com/shorts/*"
			],
			"js": [
				"unshorten.js"
			]
		}
	]
}
